@InProceedings{Ferguson2008,
  Title                    = {Motion Planning in Urban Environments: Part I},
  Author                   = {Ferguson, D. and Howard, T.M. and Likhachev, M.},
  Booktitle                = {International Conference on Intelligent Robots and Systems},
  Year                     = {2008},
  Organization             = {IEEE/RSJ},
  Pages                    = {1063--1069},

  __markedentry            = {[wgm1pal:6]},
  Abstract                 = {Literatur: Entweder Gel{\"a}ndeg{\"a}ngigkeit oder {"}very simple on-road maneuvers such as highway-based lane following {"}Boss' System ist in 4 Bl{\"o}cke gegliedert: Wahrnehmung, Missionsplanung, Verhaltensf{\"u}hrung (Verkehrsregeln, INteraktion mit anderen Fahrzeugen, .. ) Motion Planner (generiert Trajektorie) F{\"u}r die Optimierungsaufgabe wird Trajektoriengenerator aus Howard/Kelly verwendetBeschr{\"a}nkungen des Fahrzeugmodelles: kr{\"u}mmung,kr{\"u}mmungs{\"a}nderung,.beschl,verz,Fester Endpunkt aus (x,y,theta)Parametrierung u~= v und kappa, verschiedene Profile f{\"u}r vkappa: 3 Kr{\"u}mmungsparameter + Trajektorienl{\"a}nge. 1. Parameter wird festgehalten.Startwerte aus vorberechnetem Trajektorienset {\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}We present the motion planning framework for an autonomous vehicle navigating through urban environments. Such environments present a number of motion planning challenges, including ultra-reliability, high-speed operation, complex inter-vehicle interaction, parking in large unstructured lots, and constrained maneuvers. Our approach combines a model-predictive trajectory generation algorithm for computing dynamically-feasible actions with two higher-level planners for generating long range plans in both on-road and unstructured areas of the environment. In this Part I of a two-part paper, we describe the underlying trajectory generator and the on-road planning component of this system. We provide examples and results from ldquoBossrdquo, an autonomous SUV that has driven itself over 3000 kilometers and competed in, and won, the Urban Challenge.},
  File                     = {:04_Papers\\[Ferguson] Motion Planning in Urban Environments 1.pdf:PDF},
  Owner                    = {wgm1pal},
  Timestamp                = {2011.12.12}
}

@InProceedings{Ferguson2008b,
  Title                    = {Motion Planning in Urban Environments: Part II},
  Author                   = {Ferguson, D. and Howard, T.M. and Likhachev, M.},
  Booktitle                = {International Conference on Intelligent Robots and Systems},
  Year                     = {2008},
  Organization             = {IEEE/RSJ},
  Pages                    = {1070--1076},

  __markedentry            = {[wgm1pal:6]},
  Abstract                 = {Motion planning for autonomous vehicles following roads: {"}A key difference between road following and navigating unstructured environments is that in the former case a global plan is already encoded by the road (lane) itself, and therefore the planner only needs to generate short-term motion trajectories that follow the road,{"}Einteilung in Trajektorienplanung Stra{\ss}e und ungeordneter UmgebungStra{\ss}e: Ferguson et al. [30, 31] beschreibt die Bewegungsplanung f{\"u}r das autonome Fahrzeug{\{}$\backslash$glqq{\}}Boss{\{}$\backslash$grqq{\}} bei der DUC'07. Je nach Terrain (Stra{\ss}e, unstrukturierte Umgebung) kommt einanderes Planungsverfahren zum Ansatz: W{\"a}hrend die Planung in unstrukturierter Um-gebung auf einem Lattice Planner beruht, erfolgt die Planung auf der Stra{\ss}e durchein MPC-basiertes Trajektoriengenerierungsverfahren. Auf der Stra{\ss}e werden abh{\"a}ngigvom aktuellen Systemzustand eine Reihe von Endzust{\"a}nden in der n{\"a}heren Umgebungdefiniert, zu welchen jeweils eine fahrphysikalisch erlaubte Trajektorie nach dem MPCAnsatz generiert wird. Diese Trajektorien sind mit Kosten belegt: Ausgew{\"a}hlt wird dieg{\"u}nstigste hinsichtlich Abstand zu Hindernissen, Abstand zur Fahrbahnmitte, Glattheit,Fehlerma{\ss} hinsichtlich dem Erreichen des geforderten Endzustandes und Geschwindig-keit. Weiterhin erfolgt eine Kollisionsabfrage (siehe auch 4.2.2). Die Generierung desTrajektoriensets nach dem MPC-Ansatz folgt [45] und soll im Folgenden erl{\"a}utert wer-den:Die Ansatzfunktion ist dabei ein Spline 2. Ordnung, welcher sich durch die Anfangs-, diemittlere, die Endkr{\"u}mmung sowie die Bogenl{\"a}nge parametrisieren l{\"a}sst. Um m{\"o}glichstsanfte {\"U}berg{\"a}nge zu erreichen, wird die Anfangskr{\"u}mmung entsprechend dem aktuellenSystemzustand fixiert. Die {\"u}brigen 3 Parameter werden optimiert unter den Nebenbe-dingungen des geforderten Endzustandes [x,y,\textgreek{J}]end, wobei [x,y] die Position und \textgreek{J} dieAusrichtung des Fahrzeuges beschreibt. Das G{\"u}tefunktional J im betrachtet Zeitintervall[t0,tf] setzt sich u.a. zusammen aus [45]:J =[w1JRisiko+ w2JEnergie+ w3JGlattheit+ ...]dt Dabei werden Gr{\"o}{\ss}en wie das Risiko, die ben{\"o}tigte Energie, die Glattheit der Trajektoriemit den Wichtungsfaktoren wi{\"u}ber das betrachtete Zeitfenster integriert. Die Startwertef{\"u}r die Minimierung dieses G{\"u}tefunktionals werden dabei durch Interpolation innerhalbeiner Lookup-Table entnommen. {\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}{\#}We present the motion planning framework for an autonomous vehicle navigating through urban environments. Such environments present a number of motion planning challenges, including ultra-reliability, high-speed operation, complex inter-vehicle interaction, parking in large unstructured lots, and constrained maneuvers. Our approach combines a model-predictive trajectory generation algorithm for computing dynamically-feasible actions with two higher-level planners for generating long range plans in both on-road and unstructured areas of the environment. In this Part II of a two-part paper, we describe the unstructured planning component of this system used for navigating through parking lots and recovering from anomalous on-road scenarios. We provide examples and results from ldquoBossrdquo, an autonomous SUV that has driven itself over 3000 kilometers and competed in, and won, the Urban Challenge.},
  File                     = {:04_Papers\\[Ferguson] Motion Planning in Urban Environments 2.pdf:PDF},
  Owner                    = {wgm1pal},
  Timestamp                = {2011.12.12}
}

