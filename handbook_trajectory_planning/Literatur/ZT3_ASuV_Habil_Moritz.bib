@MastersThesis{galler2012,
  Title                    = {Modellbasierte Regelung zur integrierten Kollisionsvermeidung},
  Author                   = {Galler, S.},
  School                   = {Technische Universität München},
  Year                     = {2012},

  Abstract                 = {This master thesis investigates to what extent model-based control can be used
to construct an integrated collision avoidance system. As a first approach, model
predictive control (MPC) is examined, which can be used to optimize the actuating
variable u, subject to the dynamic system \dot{x} = f(x(t), u(t)) and some cost functional
J describing the optimality of the resulting trajectory
u = argmin J(u; x(t0), t_horiz).
Furthermore, it is also possible to enforce constraints to achieve collision avoidance.
While the first approach is found to be generally applicable, it cannot be solved fast
enough due to hardware limitations provided in current cars. The second approach
however fulfills the requirements and is successfully tested on an experimental vehicle.
The idea of model predictive control is motivated in section 2 and is ascribed
to the form of an optimal control problem. Different approaches to solve this
problem are introduced and discussed.
In the third section the dynamic system modelling the car and its surroundings
are introduced and the constraints to enforce the collision avoidance are
motivated.
Different approaches to solve the resulting nonlinear constrained optimization
problem are discussed and compared in the fourth section. Furthermore warmstart
methods are investigated to reduce the computing time needed and the first
approach is implemented.
Since the problem cannot be solved fast enough, in a second approach the
system is reduced to the lateral component and can therefore be embodied by
a linear dynamic system. Using this simplifications, necessary conditions to the
optimal trajectory are derived, which lead to the Riccati-Controller illustrated in
section 5.
The designed Riccati-Controller is finally optimized for pedestrian protection
and tested in section 6.},
  File                     = {Masterarbeit im PDF-Format:01_Studienabschlussarbeiten\\[Galler] Modellbasierte Regelung zur integrierten Kollisionsvermeidung.pdf:PDF},
  Keywords                 = {NMPC, Riccati-Regler, Trajektorienplanung},
  Owner                    = {q285754},
  Review                   = {[HA] Betreute Arbeiten},
  Timestamp                = {2012.09.21}
}

